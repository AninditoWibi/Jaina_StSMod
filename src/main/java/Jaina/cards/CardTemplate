    private static final int COST = XXXX;

    public CardTmp() {
        super(ID, false, CARD_STRINGS, COST, CardType.XXXX, JainaEnums.JAINA_COLOR,
                CardRarity.XXXX, CardTarget.XXXX);
    }

    @Override
    public void upp() {

    }

    @Override
    public void use(AbstractPlayer p, AbstractMonster m) {
    }

    @Override
    public AbstractCard makeCopy() {
        return new CardTmp();
    }

}
